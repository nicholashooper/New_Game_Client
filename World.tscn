script/source = "extends Node

const PLAYER_PATH = preload(\"res://player.tscn\")
const REMOTE_PLAYER_PATH = preload(\"res://remote_player.tscn\")
const PORT        = 7777
const MAX_PLAYERS = 100
var server = NetworkedMultiplayerENet.new()
var players : Dictionary = {}
var player_pawns : Dictionary = {}
var player_spawn : Vector3
var player_id : int

func _ready():
	print('Spinning up FTL drive...')
	var peer = NetworkedMultiplayerENet.new()
	peer.create_client(\"127.0.0.1\", 7777)
	get_tree().set_network_peer(peer)
	
remote func add_player(id):
	players[str(id)] = {\"loc\" : Vector2()}
	var new_pawn : Node
	if (id == player_id):
		new_pawn = PLAYER_PATH.instance()
	else:
		new_pawn = REMOTE_PLAYER_PATH.instance()
	player_pawns[id] = new_pawn;
	new_pawn.name = str(id)
	new_pawn.position = Vector2(128,128)
	get_node(\"/root/World/players\").add_child(new_pawn)

remote func identify_self(id):
	player_id = id
	rpc_id(1,\"request_playerlist\")
	print(\"SELF: \" + str(player_id))

remote func playerlist(data):
	var playerlist : Array = data
	if (playerlist.has(str(player_id))):
		playerlist.erase(str(player_id))
	
	for player in playerlist:
		var new_pawn : Node
		new_pawn = REMOTE_PLAYER_PATH.instance()
		player_pawns[player] = new_pawn;
		new_pawn.name = str(player)
		new_pawn.position = Vector2(128,128)
		get_node(\"/root/World/players\").add_child(new_pawn)

remote func rem_player(id):
	players.erase(str(id))
	player_pawns[id].queue_free();
	player_pawns.erase(id);"

